[alias]
    aa = add --all
    br = branch
    ca = commit --amend
    ci = commit
    cm = commit -m
    co = checkout
    df = diff
    dw = diff -w
    dc = diff --cached
    lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
    la = log --all --color --decorate --graph --oneline
    lo = log --oneline
    p = push
    up = pull
    st = status
    ss = status --short -b
    undo = checkout --
    hard = reset --hard
